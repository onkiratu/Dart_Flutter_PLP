class Student {
  String name;
  int age;
  String gradeLevel;

  Student(this.name, this.age, this.gradeLevel);

  void printInfo() {
    print("Student Information: Name: $name, Age: $age, Grade Level: $gradeLevel");
  }
}

class Teacher {
  String name;
  int age;
  String subject;

  Teacher(this.name, this.age, this.subject);

  void printInfo() {
    print("Teacher Information: Name: $name, Age: $age, Subject: $subject");
  }
}

class School {
  void displayInformation() {
    // Creating student and teacher objects
    Student student = Student("John Doe", 15, "10th Grade");
    Teacher teacher = Teacher("Jane Smith", 34, "Mathematics");

    // Calling methods to print information
    student.printInfo();
    teacher.printInfo();
  }
}

void main() {
  School school = School();
  school.displayInformation();
}
